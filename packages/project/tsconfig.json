{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "lib": [
      "dom",
      "es2015"
    ],
    "inlineSourceMap": false,
    "module": "esnext",
    "outDir": "lib",
    "sourceMap": false,
    "target": "es5"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}